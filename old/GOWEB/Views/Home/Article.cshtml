                                PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + i + "' > " + i + "</a></li>";
                            }
                        }
                        else if (i < End) {
                            PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + i + "' > " + i + "</a></li>";
                        }
                        else if (i = End) {
                            PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + i + "' > " + i + "</a></li>";
                            if (i < (parseInt(data) - 5)) {
                                PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + parseInt(i + 1) + "' ><i class='fa fa-chevron-right' aria-hidden='true'></i></a></li>";
                            }
                        }
                    }

                    $("#PageNumber").append(PagesItem)


                    PageNum = "<h5 class='padding-15'>صفحه " + start + " از " + data + "</h5>";
                    $("#pagenum").append(PageNum)

                });





            }
            else if (pathname.toLowerCase().indexOf("magazine") >= 0) {
                /////////////////////////magazine

                var n = pathname.lastIndexOf('/');
                var nMagazineID = pathname.substring(n + 1);

                $.getJSON("/DT/GetAllNewsMagazine/", { id: nMagazineID, start: 0 }, function (data) {
                    var $xml = $(data);
                    $.each($xml, function (index, value) {
                        var Prehref = "http://www.greenoptimizer.com/Home/ArticleDetails/" + value.NewsID + "|" + value.NTitle.replace(" ", "-").replace("+", "-").replace("?", "-").replace("*", "-").replace(";", "-").replace(",", "-").replace(".", "-").replace(":", "-").replace("؛", "-").replace("؟", "-").replace("»", "-").replace("«", "-").replace("!", "-").replace("(", "-").replace(")", "-").replace("{", "-").replace("}", "-").replace("[", "-").replace("]", "-");
                        var newesItem = "<div class='blog_list bg-white'><div class='col-md-12 blog_left'><a target='_blank' href='" + Prehref + "'><img src='' alt='' class='img-responsive' /></a><h4 class='resp-padding-5'><a target='_blank' href='" + Prehref + "'>" + value.NTitle + "</a></h4><span>نوشته شده در تاریخ " + value.PubDate + "  در " + value.SiteTitle + " <a><i class='fa fa-eye'></i></a> " + value.ViewNumber + " بازدید</span><p class='para'>" + value.Description + "</p><div class='read_btn padding-bottom10'><a target='_blank' href='" + Prehref + "'><button class='btnArticle'>ادامه مطلب</button></a></div></div><div class='clearfix'></div></div>";
                        $("#CenterContent").append(newesItem)
                    });
                });


                $.getJSON("/DT/GetAllPages/" + nMagazineID, function (data) {

                    var PagesItem = "";
                    var PageNum = "";
                    for (var i = 0; i < data && i < 11; i++) {
                        if (i < 10) {
                            PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + i + "' > " + i + "</a></li>";
                        }
                        else if (i = 10) {
                            PagesItem += "<li><a href='http://www.greenoptimizer.com/Home/MagazineSelected/" + nMagazineID + "-" + i + "' ><i class='fa fa-chevron-right' aria-hidden='true'></i></a></li>";
                        }
                    }
                    $("#PageNumber").append(PagesItem)

                    PageNum = "<h5 class='padding-15'>صفحه 0 از " + data + "</h5>";
                    $("#pagenum").append(PageNum)
                });
            }


        });
